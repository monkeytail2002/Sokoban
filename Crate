package sokoban;

import javax.swing.ImageIcon;
import java.awt.Image;

public class Crate extends MoveableMapElement {
	
	public void createCrate() {
		
		ImageIcon crate = new ImageIcon("src/resource/WarehouseKeeper.png");  //Takes th' crate image frae th' resoorces folder an' loads it tae a variable.
		Image crateImage = crate.getImage(); //Gits th' crate picture an' sets it tae th' image variable.
		setImage(crateImage); //Sets th' crate keeper image.

	}

}
