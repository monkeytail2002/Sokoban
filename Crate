package sokoban;

import javax.swing.ImageIcon;
import java.awt.Image;

public class Crate extends MoveableMapElement {
	
	public void createCrate() {
		
		ImageIcon crate = new ImageIcon("src/resource/WarehouseKeeper.png");  //Takes the crate image from the resources folder and loads it to a variable.
		Image crateImage = crate.getImage(); //Gets the crate picture and sets it to the image variable
		setImage(crateImage); //Sets the crate image

	}

}
