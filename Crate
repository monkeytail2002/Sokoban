package sokoban;

import javafx.scene.image.Image;

public class Crate extends MoveableMapElement {
	
	public void createCrate() {
		
    	Image crate = new Image("res/Crate.png"); //Takes the crate image from the resources folder and loads it to a variable.  

	}
	
	public void crateInPlace() {
		
		Image placedCrate = new Image("res/CrateInPlace.png"); //Takes the placed crate image from the resources folder and loads it to a variable.  

	}

}
