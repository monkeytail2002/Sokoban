package sokoban;

import javax.swing.ImageIcon;
import java.awt.Image;

public class WarehouseKeeper extends MoveableMapElement {
	
	public void createWarehouseKeeper() {
		
		ImageIcon crate = new ImageIcon("src/resource/WarehouseKeeper.png");  //Takes th' warehoose keeper image frae th' resoorces folder an' loads it tae a variable.
		Image crateImage = crate.getImage(); //Gits th' warehoose keeper picture an' sets it tae th' image variable.
		setImage(crateImage); //Sets th' warehoose keeper image.

	}

}
