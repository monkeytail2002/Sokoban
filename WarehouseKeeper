package sokoban;

import javax.swing.ImageIcon;
import java.awt.Image;

public class WarehouseKeeper extends MoveableMapElement {
	
	public void createWarehouseKeeper() {
		
		ImageIcon wKeeper = new ImageIcon("src/resource/WarehouseKeeper.png");  //Takes the warehouse keeper image from the resources folder and loads it to a variable.
		Image WarehouseImage = wKeeper.getImage(); //Gets the warehouse keeper picture and sets it to the image variable
		setImage(WarehouseImage); //Sets the warehouse keeper image

	}

}
