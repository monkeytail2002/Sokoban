package sokoban;

import javafx.scene.image.ImageView;

public class MoveableMapElement {
	
	Coordinates position = new Coordinates();
	WarehouseKeeper ware = new WarehouseKeeper();


	public void warehouseElement() {
		ImageView warehouse = new ImageView();
		warehouse.setImage(ware.createWarehouseKeeper());
	}
	
	public void moveElement(Coordinates Warehouse) {
		
		position.setX(1);
		position.setY(1);
		
	}
	


}
