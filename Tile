package sokoban;

import java.awt.Image;
import javax.swing.ImageIcon;

public class Tile extends MoveableMapElement{
	
	public void createTile() {
		
		ImageIcon tileSquare = new ImageIcon("src/resource/Diamond.png"); //Takes the tile image from the resources folder and loads it to a variable.
		Image tileImage = tileSquare.getImage();  //Gets the tile picture and sets it to the image variable
		setImage(tileImage); //Sets the tile image

	}

}
