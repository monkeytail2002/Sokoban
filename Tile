package sokoban;

import java.awt.Image;
import javax.swing.ImageIcon;

public class Tile extends MoveableMapElement{
	
	public void createTile() {
		
		ImageIcon tileSquare = new ImageIcon("src/resource/Diamond.png"); //Taks th' tile image frae th' resoorces folder an' loads it tae a variable.
		Image tileImage = tileSquare.getImage();  //Gits th' tile picture an' sets it tae th' image variable.
		setImage(tileImage); //Sets th' tile image.

	}

}
