package sokoban;

import java.awt.Image;
import javax.swing.ImageIcon;

public class Wall extends MapElement {
	
	private Image image; 

	
	public void createWall() {
		
		ImageIcon wallSquare = new ImageIcon("src/resource/Wall.png");  //Awrite hen, Ah've taken a picture ay a dyke an' loaded it frae th' resoorces folder.
		image = wallSquare.getImage();  //Gits th' dyke image an' sets it tae a variable.
		setImage(image); //Sets image, ye ken.

	}

}
