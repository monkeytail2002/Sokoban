package sokoban;

import java.awt.Image;
import javax.swing.ImageIcon;

public class Wall extends MapElement {
	
	private Image image; 

	
	public void createWall() {
		
		ImageIcon wallSquare = new ImageIcon("src/resource/Wall.png");  //Takes the wall image from the resources folder and loads it to a variable..
		image = wallSquare.getImage();  //Gets the wall picture and sets it to the image variable
		setImage(image); //Sets the wall image

	}

}
