package sokoban;

public class Coordinates {
	
	protected int x;
	protected int y;
	
	public Coordinates() {
		x = 0;
		y = 0;
	}
	
	public Coordinates(int newX, int newY) {
		newX = x;
		newY = y;
	}
	
	public int getX() {
		return x;
	}
	
	public int getY() {
		return y;
	}
	
	public void setX(int newX) {
		x = newX;
	}
	
	public void setY(int newY) {
		y = newY;
	}
}
